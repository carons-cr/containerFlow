<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/out_listview"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:background="@drawable/out_list_bg"
    android:gravity="center"
    android:orientation="horizontal">

    <EditText
        android:id="@+id/out_wpid"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:background="@drawable/style_textview"
        android:gravity="center"
        android:maxEms="16"
        android:maxLines="1"
        android:shadowColor="#465d45"

        android:singleLine="true"
        android:textColor="#000" />

    <EditText
        android:id="@+id/out_rqys"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:background="@drawable/style_textview"
        android:gravity="center"
        android:maxEms="4"
        android:maxLines="1"
        android:numeric="integer"
        android:textColor="#000" />

    <EditText
        android:id="@+id/out_rqxh"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:background="@drawable/style_textview_disable"
        android:editable="false"
        android:gravity="center"
        android:maxEms="16"

        android:maxLines="1"
        android:singleLine="true"
        android:textColor="#000" />

    <TextView
        android:id="@+id/sign"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="gone" />
</LinearLayout>
